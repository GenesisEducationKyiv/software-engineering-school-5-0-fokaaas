{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "types": [
      "node"
    ],
    "rootDir": "src",
    "tsBuildInfoFile": "dist/tsconfig.app.tsbuildinfo",
    "target": "es2021"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "eslint.config.mjs",
    "src/**/*.spec.ts",
    "src/**/*.ispec.ts"
  ]
}
