{
  "name": "@weather-api/weather",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build"
        }
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@weather-api/weather:build",
          "runBuildTargetDependencies": false
        }
      }
    }
  }
}
