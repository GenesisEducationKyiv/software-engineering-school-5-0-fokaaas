{
  "name": "@weather-api/subscription",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build"
        }
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@weather-api/subscription:build",
          "runBuildTargetDependencies": false
        }
      }
    }
  },
  "devDependencies": {
    "prisma": "^6.8.0"
  },
  "dependencies": {
    "@prisma/client": "^6.8.0",
    "ioredis": "^5.6.1"
  }
}
